@model HomeViewModel

<div class="container-fluid">
    <header>
        <div class="row">
            <!--logo-->
            <div class="col-xs-12 col-md-2">
                <a class="navbar-brand logo-top" title="Drogueria - La Rebaja Virtual" alt="logo - La Rebaja" href="/"><img src="/images/logotop.png" alt="La rebaja virtual"></a>
            </div>

            <div class="col-xs-12 col-md-10">
                <div class="row">
                    <div class="col-xs-2">
                        <div class="top_ubicacion">
                            Seleccionar ciudad                                            <a href="/sitio/ubicacion"><span class="text-center title-desp"><br><span class="glyphicon glyphicon-map-marker" style="margin-right: 5px;"></span> Cambiar Ciudad </span></a>
                        </div>
                    </div>
                    <div class="col-xs-6">
                        <form method="get" action="/catalogo/buscar">
                            <div class="row">
                                <div class="col-xs-7 content-search">
                                    <input type="text" class="form-control" placeholder="Escriba el nombre del producto" autocomplete="off" value="" id="busqueda" name="busqueda">
                                </div>
                                <div class="col-xs-5 content-category">
                                    <div class="controls">
                                        <span class="dropdown-toggle" data-toggle="dropdown" role="button" aria-haspopup="true" aria-expanded="false" data-label-placement="" style="width: 126px;">
                                            Todas las categorías
                                        </span>
                                        <a href="#" id="btn-buscador-productos" class="float-right"><img class="ico-buscar" src="/images/desktop/ico-buscar.png" alt=""></a>
                                        <a href="#" data-toggle="dropdown" class="dropdown-toggle float-right" style="margin-right: 2px;"><i><img src="/images/desktop/ico-filtro-categorias.png"></i></a>
                                        <ul class="dropdown-menu todas-categorias" aria-labelledby="categorias">
                                            <li>
                                                <input type="checkbox" name="categoriasBuscador[1]" id="categoriasBuscador_1" value="1">
                                                <label for="categoriasBuscador_1" class="clst_check"><span></span> <img src="/images/menu/desktop/icon_alimentos.png" alt="" class="data-label">ALIMENTOS Y BEBIDAS</label>
                                            </li>
                                            <li>
                                                <input type="checkbox" name="categoriasBuscador[47]" id="categoriasBuscador_47" value="47">
                                                <label for="categoriasBuscador_47" class="clst_check"><span></span> <img src="/images/menu/desktop/icon_aseohogar.png" alt="" class="data-label">ASEO Y HOGAR</label>
                                            </li>
                                            <li>
                                                <input type="checkbox" name="categoriasBuscador[126]" id="categoriasBuscador_126" value="126">
                                                <label for="categoriasBuscador_126" class="clst_check"><span></span> <img src="/images/menu/desktop/icon_bebes.png" alt="" class="data-label">BEBE</label>
                                            </li>
                                            <li>
                                                <input type="checkbox" name="categoriasBuscador[259]" id="categoriasBuscador_259" value="259">
                                                <label for="categoriasBuscador_259" class="clst_check"><span></span> <img src="/images/menu/desktop/icon_rehabilitacion.png" alt="" class="data-label">CUIDADO EN CASA</label>
                                            </li>
                                            <li>
                                                <input type="checkbox" name="categoriasBuscador[77]" id="categoriasBuscador_77" value="77">
                                                <label for="categoriasBuscador_77" class="clst_check"><span></span> <img src="/images/menu/desktop/icon_cuidadopersonal.png" alt="" class="data-label">CUIDADO PERSONAL</label>
                                            </li>
                                            <li>
                                                <input type="checkbox" name="categoriasBuscador[167]" id="categoriasBuscador_167" value="167">
                                                <label for="categoriasBuscador_167" class="clst_check"><span></span> <img src="/images/menu/desktop/icon_dermocosmeticos.png" alt="" class="data-label">DERMOCOSMETICA</label>
                                            </li>
                                            <li>
                                                <input type="checkbox" name="categoriasBuscador[188]" id="categoriasBuscador_188" value="188">
                                                <label for="categoriasBuscador_188" class="clst_check"><span></span> <img src="/images/menu/desktop/icon_belleza.png" alt="" class="data-label">MAQUILLAJE Y ACCESORIOS</label>
                                            </li>
                                            <li>
